{
  "name": "eslint-config-hardesty",
  "version": "7.2.2",
  "description": "Zach Hardesty's ESLint config, forked from Airbnb. To use, extend `hardesty` and optionally `hardesty/typescript`",
  "main": "index.js",
  "files": [
    "/index.js",
    "/.eslintrc.js",
    "/node.js",
    "/typescript.js",
    "/chunks"
  ],
  "scripts": {
    "prepublishOnly": "safe-publish-latest",
    "pub": "npm publish",
    "test": "tape tests/test.js",
    "lint": "eslint tests/*",
    "link:eslint": "cd node_modules/eslint && npm link --production && cd -"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zachhardesty7/eslint-config-hardesty.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "javascript",
    "styleguide",
    "es2015",
    "es2016",
    "es2017",
    "es2018"
  ],
  "author": "Zach Hardesty (https://zachhardesty.com)",
  "contributors": [
    {
      "name": "Zach Hardesty",
      "url": "https://zachhardesty.com"
    },
    {
      "name": "Jake Teton-Landis",
      "url": "https://twitter.com/jitl"
    },
    {
      "name": "Jordan Harband",
      "email": "ljharb@gmail.com",
      "url": "http://ljharb.codes"
    },
    {
      "name": "Harrison Shoff",
      "url": "https://twitter.com/hshoff"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zachhardesty7/eslint-config-hardesty/issues"
  },
  "homepage": "https://github.com/zachhardesty7/eslint-config-hardesty",
  "dependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/eslint-plugin": "^7.24.7",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "escape-regex-string": "^1.0.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.11",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-unicorn": "^53.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "eslint": "^8.57.0",
    "safe-publish-latest": "^2.0.0",
    "tape": "^5.3.1"
  },
  "peerDependencies": {
    "eslint": "^8.57.0"
  }
}
